PORT=3000
DEBUG=false
PUBLIC_USER_ID=public-user
ALLOWED_ORIGINS=http://localhost:5173
# Render production example:
# ALLOWED_ORIGINS=https://blinkstream-frontend.onrender.com
SOLANA_NETWORK=mainnet-beta
ORBITFLARE_RPC_URL=http://fra.rpc.orbitflare.com
ORBITFLARE_WS_URL=ws://fra.rpc.orbitflare.com
ORBITFLARE_GRPC_URL=http://fra.rpc.orbitflare.com:10000
ORBITFLARE_API_KEY=
# Set to false if your OrbitFlare plan rejects accountInclude program filters.
ORBITFLARE_GRPC_USE_PROGRAM_FILTER=true
# Reconnect backfill controls (recover missed swaps after stream drops).
ORBITFLARE_BACKFILL_ENABLED=true
ORBITFLARE_BACKFILL_LIMIT=25
ORBITFLARE_BACKFILL_BATCH_SIZE=8
# Optional comma-separated addresses to backfill signatures from.
ORBITFLARE_BACKFILL_ADDRESSES=So11111111111111111111111111111111111111112,EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
# Optional default address for explorer endpoints/UI.
ORBITFLARE_EXPLORER_ADDRESS=
# OrbitFlare Customer API (Ops guardrails: license + whitelist checks).
ORBITFLARE_CUSTOMER_API_BASE_URL=
ORBITFLARE_CUSTOMER_API_KEY=
ORBITFLARE_CUSTOMER_BEARER=
ORBITFLARE_CUSTOMER_LICENSES_PATH=/customer/licenses
ORBITFLARE_CUSTOMER_WHITELIST_PATH=/customer/whitelisted-ips
ORBITFLARE_PUBLIC_IP_URL=https://api.ipify.org?format=json
ORBITFLARE_OPS_MONITOR_INTERVAL_MS=60000
ORBITFLARE_OPS_MONITOR_TIMEOUT_MS=5000
# Optional OrbitFlare trading quote endpoint (falls back to JUPITER_API_URL automatically):
# ORBITFLARE_TRADING_API_URL=https://<orbitflare-trading-endpoint>/swap/v1/quote
ORBITFLARE_TRADING_API_URL=
# Alias (if your OrbitFlare docs use this naming):
ORBITFLARE_JUPITER_API_URL=
PYTH_HERMES_URL=https://hermes.pyth.network
JUPITER_API_URL=https://lite-api.jup.ag/swap/v1/quote
# Optional override for action endpoint used in generated blink URLs (must be public https URL for Dial).
BLINK_ACTION_URL=
# Optional blink wrapper host (set empty to return raw action URL)
BLINK_WRAPPER_URL=https://dial.to
BLINK_ICON_URL=https://solana.com/src/img/branding/solanaLogoMark.svg
JUPITER_INPUT_MINT=So11111111111111111111111111111111111111112
JUPITER_OUTPUT_MINT=EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v
DEFAULT_SWAP_AMOUNT=100000000
DEFAULT_INPUT_DECIMALS=9
HACKATHON_SIM_PUBLIC_KEY=
SUPPORTED_TOKENS=SOL,USDC,BONK,JUP
# Supports symbols (SOL,BONK,...) and direct mint addresses.
AUTONOMOUS_TOKENS=SOL,BONK,JUP
# Optional overrides for trade mint mapping used by blink generation:
# TOKEN_MINT_MAP=SOL:So11111111111111111111111111111111111111112,USDC:EPjFWdd5AufqSSqeM2qN1xzybapC8G4wEGGkZwyTDt1v,BONK:DezXAZ8z7PnrnRJjz3wXBoRgixCa6xjnB7YaB1pPB263,JUP:JUPyiwrYJFskUPiHa7hkeR8VUtAeFoSYbKedZNsDvCN
TOKEN_MINT_MAP=
# Optional overrides for token decimals used to convert UI amount -> raw amount:
# TOKEN_DECIMALS_MAP=SOL:9,USDC:6,BONK:5,JUP:6
TOKEN_DECIMALS_MAP=
LARGE_SWAP_USD_THRESHOLD=10000
SURGE_THRESHOLD_PERCENT=3
SURGE_COOLDOWN_MS=30000
AUTONOMOUS_POLL_MS=5000
STREAM_RECONNECT_MS=3000
# Supabase is optional in hackathon mode and not required for auth/blink storage now.
SUPABASE_URL=
SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
DEMO_MODE=false
